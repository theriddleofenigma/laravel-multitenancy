{
    "name": "theriddleofenigma/laravel-multitenancy",
    "description": "Make your Laravel/Lumen app usable by multiple tenants",
    "keywords": [
        "theriddleofenigma",
        "laravel-multitenancy",
        "lumen-multitenancy"
    ],
    "homepage": "https://github.com/theriddleofenigma/laravel-multitenancy",
    "license": "MIT",
    "authors": [
        {
            "name": "Kumaravel",
            "email": "kumarwindows11@gmail.com",
            "homepage": "https://github.com/theriddleofenigma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^8.0",
        "theriddleofenigma/laravel-package-tools": "^0.1"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.23|^7.0",
        "phpunit/phpunit": "^9.4",
        "spatie/valuestore": "^1.2",
        "laravel/octane": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Multitenancy\\": "src",
            "Spatie\\Multitenancy\\Database\\Factories\\": "database/factories"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Multitenancy\\Tests\\": "tests"
        },
        "classmap": [
            "tests/database/migrations/landlord",
            "tests/database/migrations"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Multitenancy\\MultitenancyServiceProvider"
            ],
            "aliases": {
                "Multitenancy": "Spatie\\Multitenancy\\MultitenancyFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
